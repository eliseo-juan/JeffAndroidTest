<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="64dp"
    xmlns:tools="http://schemas.android.com/tools"
    android:paddingLeft="16dp"
    android:background="?android:selectableItemBackground"
    android:focusable="true"
    android:clickable="true"
    android:paddingRight="16dp"
    android:orientation="vertical">


    <TextView
        android:id="@+id/textViewRowsuggestionGeonameTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?textAppearanceBody1"
        app:layout_constraintBottom_toTopOf="@+id/textViewRowsuggestionGeonameSubtitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        tools:text="@tools:sample/cities"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewRowsuggestionGeonameSubtitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?textAppearanceCaption"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="@tools:sample/us_zipcodes"
        app:layout_constraintTop_toBottomOf="@+id/textViewRowsuggestionGeonameTitle" />

</androidx.constraintlayout.widget.ConstraintLayout>